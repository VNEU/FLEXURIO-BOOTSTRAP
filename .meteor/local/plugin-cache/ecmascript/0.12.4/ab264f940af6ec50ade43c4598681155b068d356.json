e, tpl) {\n    e.preventDefault();\n    Session.set(\"isLogin\", true);\n  },\n  'click a.registerForm': function (e, tpl) {\n    e.preventDefault();\n    Session.set(\"isLogin\", false);\n  },\n  'click a#loginGoogle': function (e, t) {\n    e.preventDefault();\n    Session.set('isPROSES', true);\n    Meteor.loginWithGoogle({\n      requestPermissions: ['https://picasaweb.google.com/data/', 'https://www.googleapis.com/auth/userinfo.email', 'https://www.googleapis.com/auth/userinfo.profile', 'https://www.googleapis.com/auth/plus.me'],\n      requestOfflineToken: 'true'\n    }, function (error) {\n      if (error) {} else {\n        pasangkunc();\n        Router.go('/');\n      }\n    });\n  },\n  'click #loginFacebook': function (event) {\n    Session.set('isPROSES', true);\n    Meteor.loginWithFacebook({}, function (err) {\n      if (err) {\n        throw new Meteor.Error(\"Facebook login failed\");\n      } else {\n        pasangkunc();\n        Router.go('/');\n      }\n    });\n  },\n  'submit form.register-server': function (e, tpl) {\n    e.preventDefault();\n    Session.set('isPROSES', true);\n    var textFirstName = tpl.$('input[name=textFirstName]').val();\n    var textLastName = tpl.$('input[name=textLastName]').val();\n    var textEmail = tpl.$('input[name=textEmail]').val();\n    var textPassword = tpl.$('input[name=textPassword]').val();\n    var textPasswordRetype = tpl.$('input[name=textPasswordRetype]').val();\n\n    if (textPassword != textPasswordRetype) {\n      FlashMessages.sendWarning('Sorry, Please retype your password corectlly !');\n      return;\n    }\n\n    Accounts.createUser({\n      password: textPassword,\n      email: textEmail,\n      profile: {\n        name: textFirstName + ' ' + textLastName,\n        firstName: textFirstName,\n        lastName: textLastName\n      }\n    }, function (err) {\n      if (err) {\n        FlashMessages.sendWarning('Register failed, Please try again');\n      } else {\n        pasangkunc();\n        Router.go('/');\n        FlashMessages.sendSuccess('Hello ' + UserName() + ', Wellcome to ' + sAPPName);\n      }\n    });\n  },\n  'submit form.login-server': function (e, tpl) {\n    e.preventDefault();\n    var textEmail = tpl.$('input[name=username]').val();\n    var textPassword = tpl.$('input[name=password]').val();\n    Meteor.loginWithPassword(textEmail, textPassword, function (err) {\n      if (err) {\n        FlashMessages.sendWarning('Sorry, your username or password not valid !');\n      } else {\n        FlashMessages.sendSuccess('Hello ' + UserName() + ', Wellcome to ' + sAPPName);\n        pasangkunc();\n        Router.go('/');\n      }\n    });\n  }\n});","map":{"version":3,"sources":["client/views/core/login.js"],"names":["Session","module","link","v","Template","pasangkunc","Meteor","call","login","created","set","helpers","sHeaderBackground","isLogin","get","events","e","tpl","preventDefault","t","loginWithGoogle","requestPermissions","requestOfflineToken","error","Router","go","event","loginWithFacebook","err","Error","textFirstName","$","val","textLastName","textEmail","textPassword","textPasswordRetype","FlashMessages","sendWarning","Accounts","createUser","password","email","profile","name","firstName","lastName","sendSuccess","UserName","sAPPName","loginWithPassword"],"mappings":"AAAA,IAAIA,OAAJ;AAAYC,MAAM,CAACC,IAAP,CAAY,gBAAZ,EAA6B;AAACF,EAAAA,OAAD,YAASG,CAAT,EAAW;AAACH,IAAAA,OAAO,GAACG,CAAR;AAAU;AAAtB,CAA7B,EAAqD,CAArD;AAAwDF,MAAM,CAACC,IAAP,CAAY,cAAZ;AAA4B,IAAIE,QAAJ;AAAaH,MAAM,CAACC,IAAP,CAAY,mBAAZ,EAAgC;AAACE,EAAAA,QAAD,YAAUD,CAAV,EAAY;AAACC,IAAAA,QAAQ,GAACD,CAAT;AAAW;AAAxB,CAAhC,EAA0D,CAA1D;;AAQ7GE,UAAU,GAAG,YAAY;AACxBC,EAAAA,MAAM,CAACC,IAAP,CAAY,WAAZ;AACA,CAFD;;AAIAH,QAAQ,CAACI,KAAT,CAAeC,OAAf,GAAyB,YAAU;AAClCT,EAAAA,OAAO,CAACU,GAAR,CAAY,SAAZ,EAAuB,IAAvB;AACA,CAFD;;AAIAN,QAAQ,CAACI,KAAT,CAAeG,OAAf,CAAuB;AACtBC,EAAAA,iBAAiB,EAAE,YAAY;AAC9B,WAAOA,iBAAP;AACG,GAHkB;AAItBC,EAAAA,OAAO,EAAE,YAAU;AAClB,WAAOb,OAAO,CAACc,GAAR,CAAY,SAAZ,CAAP;AACA